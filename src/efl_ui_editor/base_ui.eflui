{
  "ui-name" : "Base_Ui",
  "ui-content" : {
    "type": "Efl.Ui.Panes",
    "id" : "root",
    "split_ratio" : 0.2,
    "orientation" : vertical,
    "pack-part" : [
       {
          "type" : "Efl.Ui.Stack",
          "id" : "prop_stack",
